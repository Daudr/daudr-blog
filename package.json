{
  "name": "daudr-blog",
  "private": false,
  "description": "Front end code for Daudr's blog",
  "version": "1.8.2",
  "author": "Michele Da Rin Fioretto <michele@daudr.me>",
  "bugs": {
    "url": "https://github.com/Daudr/daudr-blog/issues"
  },
  "dependencies": {
    "@loadable/component": "^5.14.1",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "gatsby": "^3.0.0",
    "gatsby-image": "^2.4.17",
    "gatsby-plugin-amp": "^0.2.2",
    "gatsby-plugin-disqus": "^1.2.2",
    "gatsby-plugin-feed": "^3.0.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-loadable-components-ssr": "^2.1.0",
    "gatsby-plugin-mailchimp": "^5.2.2",
    "gatsby-plugin-manifest": "^3.0.0",
    "gatsby-plugin-material-ui": "^2.1.10",
    "gatsby-plugin-offline": "^4.0.0",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-helmet": "^4.0.0",
    "gatsby-plugin-robots-txt": "^1.5.5",
    "gatsby-plugin-sharp": "^3.0.0",
    "gatsby-plugin-sitemap": "^3.0.0",
    "gatsby-plugin-typography": "^3.0.0",
    "gatsby-plugin-web-monetization": "^1.1.0",
    "gatsby-remark-copy-linked-files": "^3.0.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^4.0.0",
    "gatsby-remark-prismjs": "^4.0.0",
    "gatsby-remark-responsive-iframe": "^3.0.0",
    "gatsby-remark-smartypants": "^3.0.0",
    "gatsby-source-filesystem": "^3.0.0",
    "gatsby-transformer-remark": "^3.0.0",
    "gatsby-transformer-sharp": "^3.0.0",
    "postcss": "^8.2.6",
    "prismjs": "1.23.0",
    "react": "^17.0.1",
    "react-cookie-consent": "^6.2.3",
    "react-dom": "^17.0.1",
    "react-flags-select": "^2.1.2",
    "react-helmet": "^6.1.0",
    "react-share": "^4.4.0",
    "react-typography": "^0.16.19",
    "typeface-anton": "1.1.13",
    "typeface-fira-code": "^1.1.13",
    "typeface-merriweather": "1.1.13",
    "typeface-roboto": "1.1.13",
    "typography": "^0.16.19",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "@testing-library/cypress": "^7.0.4",
    "auto-changelog": "^2.2.1",
    "axe-core": "^4.1.2",
    "babel-jest": "^26.6.3",
    "babel-preset-gatsby": "^1.0.0",
    "cypress": "^6.6.0",
    "cypress-axe": "^0.12.2",
    "cz-conventional-changelog": "^3.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint-config-react-app": "^6.0.0",
    "firebase-tools": "^9.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "semantic-release": "^17.4.0",
    "start-server-and-test": "^1.12.0"
  },
  "homepage": "https://blog.daudr.me",
  "keywords": [
    "gatsby",
    "blog",
    "react"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "https://github.com/Daudr/daudr-blog.git"
  },
  "scripts": {
    "build": "gatsby build",
    "build:test": "GATSBY_ACTIVE_ENV=development gatsby build",
    "build:prod": "GATSBY_ACTIVE_ENV=production gatsby build",
    "develop": "NODE_ENV=development && gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "deploy:test": "rm -fr public/ && GATSBY_ACTIVE_ENV=\"development\" gatsby build && firebase deploy --only hosting:test",
    "deploy:prod": "rm -fr public/ && set GATSBY_ACTIVE_ENV=production && gatsby build && firebase deploy --only hosting:prod",
    "cy:run": "npx cypress run",
    "cy:open": "cypress open",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run",
    "deploy:test:ci": "firebase deploy --only hosting:test --token=$FIREBASE_TOKEN",
    "deploy:prod:ci": "firebase deploy --only hosting:prod --token=$FIREBASE_TOKEN",
    "test": "jest",
    "test:ci": "jest --ci",
    "semantic-release": "semantic-release"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@1.0.2"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
