{
  "name": "daudr-blog",
  "private": true,
  "description": "Front end code for Daudr's blog",
  "version": "1.3.0",
  "author": "Michele Da Rin Fioretto <michele@daudr.me>",
  "bugs": {
    "url": "https://github.com/Daudr/daudr-blog/issues"
  },
  "dependencies": {
    "gatsby": "^2.3.31",
    "gatsby-image": "^2.0.40",
    "gatsby-plugin-amp": "^0.2.1",
    "gatsby-plugin-feed": "^2.1.1",
    "gatsby-plugin-google-analytics": "^2.0.18",
    "gatsby-plugin-mailchimp": "^5.1.0",
    "gatsby-plugin-manifest": "^2.0.29",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sharp": "^2.0.35",
    "gatsby-plugin-sitemap": "^2.1.0",
    "gatsby-plugin-typography": "^2.2.13",
    "gatsby-remark-copy-linked-files": "^2.0.12",
    "gatsby-remark-images": "^2.0.6",
    "gatsby-remark-prismjs": "^3.2.8",
    "gatsby-remark-responsive-iframe": "^2.1.1",
    "gatsby-remark-smartypants": "^2.0.9",
    "gatsby-source-filesystem": "^2.0.32",
    "gatsby-transformer-remark": "^2.3.12",
    "gatsby-transformer-sharp": "^2.1.18",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.19",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.54",
    "typography": "^0.16.19",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "eslint-plugin-react-hooks": "^1.6.0",
    "prettier": "^1.17.0"
  },
  "homepage": "https://blog.daudr.me",
  "keywords": [
    "gatsby",
    "blog",
    "react"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Daudr/daudr-blog.git"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "NODE_ENV=development && gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "deploy:test": "gatsby build && firebase deploy --only hosting:test",
    "deploy:prod": "gatsby build && firebase deploy --only hosting:prod",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@1.0.2"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}