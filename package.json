{
  "name": "daudr-blog",
  "private": true,
  "description": "Front end code for Daudr's blog",
  "version": "0.4.1",
  "author": "Michele Da Rin Fioretto <michele@daudr.me>",
  "bugs": {
    "url": "https://github.com/Daudr/daudr-blog/issues"
  },
  "dependencies": {
    "@material-ui/core": "^4.1.3",
    "gatsby": "^2.10.5",
    "gatsby-image": "^2.2.3",
    "gatsby-plugin-amp": "^0.2.2",
    "gatsby-plugin-disqus": "^1.0.3",
    "gatsby-plugin-feed": "^2.3.1",
    "gatsby-plugin-google-analytics": "^2.1.0",
    "gatsby-plugin-mailchimp": "^5.1.0",
    "gatsby-plugin-manifest": "^2.2.0",
    "gatsby-plugin-offline": "^2.2.0",
    "gatsby-plugin-react-helmet": "^3.1.0",
    "gatsby-plugin-sharp": "^2.2.1",
    "gatsby-plugin-sitemap": "^2.2.0",
    "gatsby-plugin-typography": "^2.3.0",
    "gatsby-remark-copy-linked-files": "^2.1.0",
    "gatsby-remark-images": "^3.1.1",
    "gatsby-remark-prismjs": "^3.3.0",
    "gatsby-remark-responsive-iframe": "^2.2.1",
    "gatsby-remark-smartypants": "^2.1.0",
    "gatsby-source-filesystem": "^2.1.1",
    "gatsby-transformer-remark": "^2.5.0",
    "gatsby-transformer-sharp": "^2.2.0",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-share": "^3.0.0",
    "react-typography": "^0.16.19",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.54",
    "typography": "^0.16.19",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "auto-changelog": "^1.13.0",
    "prettier": "^1.18.2"
  },
  "homepage": "https://blog.daudr.me",
  "keywords": [
    "gatsby",
    "blog",
    "react"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Daudr/daudr-blog.git"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "NODE_ENV=development && gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "deploy:test": "rm -fr public/ && gatsby build && firebase deploy --only hosting:test",
    "deploy:prod": "rm -fr public/ && gatsby build && firebase deploy --only hosting:prod",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@1.0.2"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}
