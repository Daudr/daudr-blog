{
  "name": "daudr-blog",
  "private": false,
  "description": "Front end code for Daudr's blog",
  "version": "1.5.0",
  "author": "Michele Da Rin Fioretto <michele@daudr.me>",
  "bugs": {
    "url": "https://github.com/Daudr/daudr-blog/issues"
  },
  "dependencies": {
    "@material-ui/core": "^4.8.1",
    "@material-ui/icons": "^4.9.1",
    "gatsby": "^2.24.53",
    "gatsby-image": "^2.2.37",
    "gatsby-plugin-amp": "^0.2.2",
    "gatsby-plugin-disqus": "^1.1.4",
    "gatsby-plugin-feed": "^2.3.25",
    "gatsby-plugin-mailchimp": "^5.2.2",
    "gatsby-plugin-manifest": "^2.4.27",
    "gatsby-plugin-material-ui": "^2.1.10",
    "gatsby-plugin-offline": "^3.2.26",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-helmet": "^3.1.18",
    "gatsby-plugin-robots-txt": "^1.5.1",
    "gatsby-plugin-sharp": "^2.6.31",
    "gatsby-plugin-sitemap": "^2.4.12",
    "gatsby-plugin-typography": "^2.3.20",
    "gatsby-plugin-web-monetization": "^1.0.1",
    "gatsby-remark-copy-linked-files": "^2.3.13",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^3.3.28",
    "gatsby-remark-prismjs": "^3.5.11",
    "gatsby-remark-responsive-iframe": "^2.2.30",
    "gatsby-remark-smartypants": "^2.1.19",
    "gatsby-source-filesystem": "^2.3.27",
    "gatsby-transformer-remark": "^2.8.32",
    "gatsby-transformer-sharp": "^2.5.14",
    "prismjs": "1.21.0",
    "react": "^16.12.0",
    "react-cookie-consent": "^5.1.3",
    "react-dom": "^16.12.0",
    "react-flags-select": "^1.1.13",
    "react-helmet": "^6.1.0",
    "react-share": "^4.2.1",
    "react-typography": "^0.16.19",
    "typeface-anton": "0.0.72",
    "typeface-merriweather": "0.0.72",
    "typeface-roboto": "0.0.75",
    "typography": "^0.16.19",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "@testing-library/cypress": "^7.0.0",
    "auto-changelog": "^2.2.0",
    "axe-core": "^4.0.1",
    "babel-jest": "^26.3.0",
    "babel-preset-gatsby": "^0.5.8",
    "cypress": "^5.1.0",
    "cypress-axe": "^0.8.1",
    "cz-conventional-changelog": "^3.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "eslint-config-react-app": "^5.1.0",
    "firebase-tools": "^8.10.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "prettier": "^2.1.1",
    "react-test-renderer": "^16.12.0",
    "semantic-release": "^17.1.1",
    "start-server-and-test": "^1.10.6"
  },
  "homepage": "https://blog.daudr.me",
  "keywords": [
    "gatsby",
    "blog",
    "react"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "https://github.com/Daudr/daudr-blog.git"
  },
  "scripts": {
    "build": "gatsby build",
    "build:test": "GATSBY_ACTIVE_ENV=development gatsby build",
    "build:prod": "GATSBY_ACTIVE_ENV=production gatsby build",
    "develop": "NODE_ENV=development && gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "deploy:test": "rm -fr public/ && GATSBY_ACTIVE_ENV=\"development\" gatsby build && firebase deploy --only hosting:test",
    "deploy:prod": "rm -fr public/ && set GATSBY_ACTIVE_ENV=production && gatsby build && firebase deploy --only hosting:prod",
    "cy:run": "npx cypress run",
    "cy:open": "cypress open",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run",
    "deploy:test:ci": "firebase deploy --only hosting:test --token=$FIREBASE_TOKEN",
    "deploy:prod:ci": "firebase deploy --only hosting:prod --token=$FIREBASE_TOKEN",
    "test": "jest",
    "test:ci": "jest --ci",
    "semantic-release": "semantic-release"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@1.0.2"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
